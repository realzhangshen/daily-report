# Google API Key for Gemini LLM
GOOGLE_API_KEY=your_key_here
# OpenAI API Key for OpenAI-compatible providers
OPENAI_API_KEY=your_key_here

# Langfuse Tracing (optional)
LANGFUSE_PUBLIC_KEY=your_langfuse_public_key
LANGFUSE_SECRET_KEY=your_langfuse_secret_key
LANGFUSE_BASE_URL=http://localhost:3000

# Crawl4AI Remote API (REQUIRED)
CRAWL4AI_API_URL=https://your-crawl4ai-api-server.com

# Crawl4AI API Basic Auth (for nginx auth)
CRAWL4AI_API_USERNAME=your_username
CRAWL4AI_API_PASSWORD=your_password
